#include <stdio.h>
#include <stdlib.h>

/**
 *main - print name of file
 *@argc: argument count
 *@argv: number of arguments compiled
 *Return: 0 if success
 */

int main(int argc, char *argv[])
{
	int i;
	int sum = 0;

	if (argc == 1)
	{
		printf("0\n");
		return (0);
	}
	for (i = 0; i < argc; i++)
	{
		sum += atoi(argv[i]);
			if !(atoi(argv[i] >= 0)
				{
			     printf("Error\n");
			     return (1);
				}
				else
					printf("%d\n", sum)
						}}
}
